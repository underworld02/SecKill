<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.suron.ysyliving.commodity.dao.CategoryDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.suron.ysyliving.commodity.entity.CategoryEntity" id="categoryMap">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="parentId" column="parent_id"/>
        <result property="catLevel" column="cat_level"/>
        <result property="isShow" column="is_show"/>
        <result property="sort" column="sort"/>
        <result property="icon" column="icon"/>
        <result property="proUnit" column="pro_unit"/>
        <result property="proCount" column="pro_count"/>
    </resultMap>

    <!--如果后面我们CategoryDao有自定义的方法，我们在配置实现即可-->
</mapper>